{
    "name": "arbeidsrettet-dialog",
    "version": "1.0.0",
    "homepage": "/arbeid/dialog",
    "license": "MIT",
    "repository": "https://github.com/navikt/arbeidsrettet-dialog.git",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "vite",
        "build:dev-ekstern": "vite build --mode=dev-ekstern --base=/arbeid/dialog/",
        "build:dev-intern": "vite build --mode=dev-intern --base=https://arbeidsrettet-dialog.intern.dev.nav.no/",
        "build:prod-ekstern": "vite build --mode=prod-ekstern --base=/arbeid/dialog/",
        "build:prod-intern": "vite build --mode=prod-intern --base=https://arbeidsrettet-dialog.intern.nav.no/",
        "build:gh-pages": "VITE_USE_HASH_ROUTER=true vite build --mode=dev-ekstern --base=/arbeidsrettet-dialog/",
        "build": "vite build",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "test:coverage": "npm test -- --coverage --watchAll=false",
        "format-all": "prettier --write 'src/'",
        "prettier-check": "prettier --check 'src/'",
        "lint": "echo \"Not implemented\"",
        "lint-staged": "lint-staged"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,json,less,css,md}": [
            "prettier --write"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "dependencies": {
        "@hookform/resolvers": "^3.1.0",
        "@navikt/aksel-icons": "^2.9.4",
        "@navikt/ds-css": "^2.9.4",
        "@navikt/ds-react": "^2.9.4",
        "@navikt/ds-tailwind": "^2.9.4",
        "@navikt/navspa": "6.0.1",
        "@vitejs/plugin-react-swc": "^3.3.0",
        "amplitude-js": "^8.21.9",
        "babel-polyfill": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "date-fns": "^2.30.0",
        "less": "^4.1.3",
        "lint-staged": "^13.2.2",
        "lodash.throttle": "^4.1.1",
        "prop-types": "^15.8.1",
        "query-string": "^8.1.0",
        "react": "^18.2.0",
        "react-collapse": "^5.1.1",
        "react-dom": "^18.2.0",
        "react-flip-toolkit": "^7.0.17",
        "react-hook-form": "^7.43.9",
        "react-router": "^6.11.0",
        "react-router-dom": "^6.11.0",
        "react-textarea-autosize": "^8.4.1",
        "smoothscroll-polyfill": "^0.4.4",
        "uuid": "^9.0.0",
        "vite": "^4.3.4",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-svgr": "^3.2.0",
        "whatwg-fetch": "^3.6.2",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@testing-library/react": "^14.0.0",
        "@types/amplitude-js": "^8.16.2",
        "@types/node": "^18.16.3",
        "@types/react": "^18.2.5",
        "@types/react-collapse": "^5.0.1",
        "@types/react-dom": "^18.2.3",
        "@types/uuid": "^9.0.1",
        "autoprefixer": "^10.4.14",
        "classnames": "^2.3.2",
        "cross-env": "^7.0.3",
        "husky": "^8.0.3",
        "import-sort-style-module": "^6.0.0",
        "jsdom": "^22.0.0",
        "msw": "^1.2.1",
        "postcss": "^8.4.23",
        "prettier": "^2.8.8",
        "prettier-plugin-import-sort": "0.0.7",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.0.4",
        "vitest": "^0.30.1"
    },
    "msw": {
        "workerDirectory": "public"
    }
}
