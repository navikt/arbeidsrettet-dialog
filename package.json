{
    "name": "arbeidsrettet-dialog",
    "version": "1.0.0",
    "homepage": "/arbeid/dialog",
    "license": "MIT",
    "repository": "https://github.com/navikt/arbeidsrettet-dialog.git",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "vite",
        "build:dev-ekstern": "vite build --mode=dev-ekstern",
        "build:dev-intern": "vite build --mode=dev-intern",
        "build:prod-ekstern": "vite build --mode=prod-ekstern",
        "build:prod-intern": "vite build --mode=prod-intern",
        "build:gh-pages": "VITE_USE_HASH_ROUTER=true vite build --mode=dev-ekstern --base=/arbeidsrettet-dialog/",
        "dev:test": "VITE_USE_HASH_ROUTER=true vite --mode=dev-ekstern --base=/arbeidsrettet-dialog/",
        "build": "vite build",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "test:coverage": "npm test -- --coverage --watchAll=false",
        "format-all": "prettier --write 'src/'",
        "prettier-check": "prettier --check 'src/'",
        "lint": "echo \"Not implemented\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,json,less,css,md}": [
            "prettier --write"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "dependencies": {
        "@hookform/resolvers": "^3.1.0",
        "@navikt/aksel-icons": "^2.8.12",
        "@navikt/ds-css": "^2.8.12",
        "@navikt/ds-react": "^2.8.12",
        "@navikt/ds-tailwind": "^2.8.13",
        "@navikt/navspa": "5.0.1",
        "@vitejs/plugin-react-swc": "^3.3.0",
        "amplitude-js": "^8.19.0",
        "babel-polyfill": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "date-fns": "^2.12.0",
        "less": "^4.1.3",
        "lint-staged": "^10.2.2",
        "lodash.throttle": "^4.1.1",
        "prop-types": "^15.7.2",
        "query-string": "^6.12.1",
        "react": "^17.0.1",
        "react-collapse": "^5.0.1",
        "react-dom": "^17.0.1",
        "react-flip-toolkit": "^7.0.9",
        "react-hook-form": "^7.43.9",
        "react-modal": "^3.11.2",
        "react-router": "^6.10.0",
        "react-router-dom": "^6.10.0",
        "react-scripts": "^4.0.3",
        "react-textarea-autosize": "^8.3.1",
        "smoothscroll-polyfill": "^0.4.4",
        "uuid": "^8.0.0",
        "vite": "^4.2.1",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-svgr": "^2.4.0",
        "whatwg-fetch": "^3.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@testing-library/react": "^12.1.5",
        "@types/amplitude-js": "^8.16.2",
        "@types/classnames": "^2.2.10",
        "@types/node": "^13.13.5",
        "@types/react": "^16.9.34",
        "@types/react-collapse": "^5.0.0",
        "@types/react-dom": "^16.9.7",
        "@types/react-modal": "^3.12.0",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-textarea-autosize": "^4.3.5",
        "@types/uuid": "^7.0.3",
        "autoprefixer": "^10.4.14",
        "classnames": "^2.2.6",
        "cross-env": "^7.0.2",
        "husky": "^4.2.5",
        "import-sort-style-module": "^6.0.0",
        "jsdom": "^21.1.1",
        "msw": "^1.2.1",
        "postcss": "^8.4.22",
        "prettier": "^2.1.2",
        "prettier-plugin-import-sort": "0.0.6",
        "tailwindcss": "^3.3.1",
        "typescript": "^5.0.4",
        "vitest": "^0.30.1"
    },
    "msw": {
        "workerDirectory": "public"
    }
}
